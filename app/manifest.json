{
  "name": "filter results older then a year when searching ubuntu topics",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "filter results older then a year when searching ubuntu topics",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "filter results older then a year when searching ubuntu topics",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "omnibox": {
    "keyword": "filter results older then a year when searching ubuntu topics"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "tabs",
    "history",
    "management",
    "http://*/*",
    "https://*/*",
    "*://*.google.com/*"
  ]
}
